<h1>Create a Store</h1>
<%= form_for @store do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br />
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br />
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.submit %>
  <% end %>

  <p>
  <%= link_to 'Home Page', root_path %>
</p>